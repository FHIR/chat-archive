an class=\"s2\">&quot;text&quot;</span> <span class=\"o\">:</span> <span class=\"s2\">&quot;Blood Group&quot;</span>\n    <span class=\"p\">},</span>\n    <span class=\"s2\">&quot;valueString&quot;</span> <span class=\"o\">:</span> <span class=\"s2\">&quot;&lt;A.ABO&gt;&quot;</span>\n  <span class=\"p\">},{</span>\n    <span class=\"s2\">&quot;code&quot;</span> <span class=\"o\">:</span> <span class=\"p\">{</span>\n      <span class=\"s2\">&quot;coding&quot;</span> <span class=\"o\">:</span> <span class=\"p\">[{</span>\n        <span class=\"s2\">&quot;system&quot;</span> <span class=\"o\">:</span> <span class=\"s2\">&quot;http://loinc.org&quot;</span><span class=\"p\">,</span>\n        <span class=\"s2\">&quot;code&quot;</span> <span class=\"o\">:</span> <span class=\"s2\">&quot;10331-7&quot;</span>\n      <span class=\"p\">}],</span>\n    <span class=\"s2\">&quot;text&quot;</span> <span class=\"o\">:</span> <span class=\"s2\">&quot;Rh Status&quot;</span>\n    <span class=\"p\">},</span>\n    <span class=\"s2\">&quot;valueString&quot;</span> <span class=\"o\">:</span> <span class=\"s2\">&quot;&lt;A.Rh&gt;&quot;</span>\n  <span class=\"p\">}]</span>\n<span class=\"p\">}</span>\n</pre></div>",
